{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [ 
        "prettier"
    ],
    "rules": {
        "no-underscore-dangle": ["error", { "allow": ["_id"]}],
        "prettier/prettier": ["error",
            {
              "endOfLine": "auto"
            }
          ],
        "no-unused-vars": "off",
        "consistent-return": "off"
    }
}
